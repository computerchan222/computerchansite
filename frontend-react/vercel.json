{
  'version': 2,
  'cleanUrls': true,
  'headers': [
    {
      'source': '**/*.js',
      'headers': [
        {
          'key': 'Content-Type',
          'value': 'application/javascript'
        }
      ]
    },
    {
      'source': '**/*.jsx',
      'headers': [
        {
          'key': 'Content-Type',
          'value': 'application/javascript'
        }
      ]
    }
  ],
  'rewrites': [
    { 'source': '/api/(.*)', 'destination': 'https://computerchansite-production.up.railway.app/api/' }
  ]
}
